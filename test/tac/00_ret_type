class a{
    x:int;
    z:link b;
    y:int;
    a f(i:b,j:int){
        this.y;
        this.y=this.y+i.x(j); //this.y = 545
        x:new b;
        if(this.y){
            //this.y=this.y+i.x(j);
            z:int;
            z=this.y-45;//500
            this.x=z;//500
            return this;
        }
        return this;
    }
}

class b{
    int x(i:int){return 233+i;}
}

